#!/bin/bash
#PBS -l nodes=1:ppn=4,vmem=32gb,walltime=11:00:00
#PBS -N tractanalysisprofiles
#PBS -V

singularity exec -e docker://brainlife/mcr:neurodebian1604-r2017a ./compiled/main

count=$(ls images/*.png | wc -l)
NUMFILES=`cat numfiles.txt`
#cp product.json ./profiles/
rm -rf product.json
if [ "$count" !=  "$NUMFILES" ];
then
	echo "not all output generated"
	exit 1
fi

# generate summary csv tractmeasures output
singularity exec -e docker://brainlife/freesurfer-stats:1.2 ./generateCSV.py
